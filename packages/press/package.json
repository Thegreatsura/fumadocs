{
  "name": "fumapress",
  "version": "0.0.0",
  "description": "The minimal setup for Fumadocs",
  "keywords": [
    "Fumadocs",
    "Docs"
  ],
  "homepage": "https://fumadocs.dev",
  "repository": "github:fuma-nama/fumadocs",
  "license": "MIT",
  "author": "Fuma Nama",
  "type": "module",
  "files": [
    "app/*",
    "dist/*"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "react-router": "^7.9.3",
    "@react-router/dev": "^7.9.3",
    "@react-router/serve": "^7.9.3",
    "@vitejs/plugin-rsc": "^0.4.32",
    "tailwindcss": "^4.1.13",
    "@tailwindcss/vite": "^4.1.13",
    "vite": "^7.1.7",
    "fumadocs-core": "workspace:*",
    "fumadocs-mdx": "workspace:*",
    "fumadocs-ui": "workspace:*"
  },
  "devDependencies": {
    "tsconfig": "workspace:*",
    "eslint-config-custom": "workspace:*",
    "@types/mdx": "^2.0.13",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.14",
    "@types/react-dom": "^19.1.9",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "@types/react": "*"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
